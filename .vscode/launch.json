{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) Launch",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/prefix/bin/w3m.exe",
            "args": [
                "http://www.yahoo.co.jp",
            ],
            "environment": [
                {
                    "name": "PATH",
                    "value": "${env:PATH};D:\\msys64\\usr\\bin"
                }
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal"
        },
        {
            "name": "(Windows) tui",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/prefix/bin/tui_sample.exe",
            "args": [],
            "environment": [
                {
                    "name": "PATH",
                    "value": "${env:PATH};D:\\msys64\\usr\\bin"
                }
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal"
        },
        {
            "name": "(lldb) 起動",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/prefix/bin/w3m",
            "args": [
                //"http://www.yahoo.co.jp",
                "http://www.google.com",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "lldb"
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/prefix/bin/w3m",
            "args": [
                // "http://www.yahoo.co.jp"
                "http://www.google.com",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}